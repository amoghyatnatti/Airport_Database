@model IEnumerable<Airport_Database.Models.TrafficController>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.MostRecentExamDate)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.SsnNavigation)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.MostRecentExamDate)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.SsnNavigation.Name)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.Ssn">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.Ssn">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.Ssn">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
